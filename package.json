{
  "name": "parcel-test",
  "version": "1.0.0",
  "scripts": {
    "dev": "npm run build && webpack serve",
    "serve": "webpack serve --mode=development --progress",
    "start": "npm run build && node ./server.js",
    "build": "webpack --mode=production --node-env=production",
    "lint": "eslint --fix",
    "stylelint": "npm run stylelint",
    "eslint:1": "./node_modules/.bin/eslint . --fix",
    "test": "mocha --require ts-node/register --require ./mochaSetup.js './src/**/*.test.ts'",
    "prepare": "husky install"
  },
  "engines": {
    "node": "^16.13.0"
  },
  "targets": {
    "default": {
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    }
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "license": "ISC",
  "dependencies": {
    "express": "4.18.1",
    "express-history-api-fallback": "2.2.1",
    "nanoid": "3.3.4",
    "parcel": "2.7.0"
  },
  "devDependencies": {
    "@anikin/parcel-transformer-handlebars-precompile": "1.0.1",
    "@parcel/transformer-sass": "2.7.0",
    "@types/chai": "4.3.3",
    "@types/jsdom": "20.0.0",
    "@types/mocha": "9.1.1",
    "@types/proxyquire": "1.3.28",
    "@types/sinon": "10.0.13",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "chai": "4.3.6",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.1",
    "eslint-config-airbnb": "19.0.4",
    "handlebars": "4.7.7",
    "handlebars-loader": "1.7.2",
    "html-webpack-plugin": "5.5.0",
    "husky": "^8.0.1",
    "jsdom": "20.0.1",
    "mini-css-extract-plugin": "2.6.1",
    "mocha": "10.0.0",
    "proxyquire": "2.1.3",
    "sass": "1.54.4",
    "sass-loader": "13.1.0",
    "sinon": "14.0.1",
    "style-loader": "3.3.1",
    "terser-webpack-plugin": "5.3.6",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "typescript": "4.7.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1"
  }
}
